<view class="container">
  <swiper indicator-dots="{{indicatorDots}}"
    autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}">
    <block wx:for="{{imgUrls}}" wx:key="id" class="block">
      <swiper-item>
        <image src="{{item}}" class="image"/>
      </swiper-item>
    </block>
  </swiper>
  <view class="name">
    项目名称：
